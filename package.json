{
  "name": "fastapi",
  "version": "0.1.0",
  "description": "FastAPI is a powerful library that simplifies the creation of RESTful APIs using Fastify and Sequelize. It allows you to define your database tables and API routes in a JSON file, making the development process faster and more efficient.",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.js*' --write",
    "prettier-watch": "onchange 'src/**/*.js*' -- prettier --write {{changed}}"
  },
  "author": "Philippe Assis <codephilippe@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.2.1",
    "fastify": "4.17.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.31.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^2.8.8"
  }
}
